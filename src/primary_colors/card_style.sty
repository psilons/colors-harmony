\usepackage{tikz}

\newcommand{\colordisk}[2] {
    \null
    \vfill
    \begin{center}
        \begin{tikzpicture}
            \filldraw[color={#1}, fill={#1}](0,0) circle (3.5in);
            \filldraw[color={#2}, fill={#2}](0,0) circle (0.5in);
        \end{tikzpicture}
    \end{center}
    \null
    \vfill
}

